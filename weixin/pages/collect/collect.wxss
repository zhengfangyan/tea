/* pages/collect/collect.wxss */
page{  
  background: #f3f4f5;  
}  
 .cart{
  margin:60rpx 0 0 60rpx;
  border-radius: 60%;
  width: 600rpx;
  height: 600rpx;
  background:  url(http://127.0.0.1:3000/img/goods/zw.gif) no-repeat;
  background-size: 100%;
}  
.empty-text {
  font-size:40rpx;
  color: #999;
  margin: 20rpx 0 0 230rpx;
}
.collect_content{
  display: flex;
  margin:20rpx 0rpx 20rpx 0rpx;
  position: relative;
}

.collect_content .item-image{
  width:200rpx;
  height:200rpx;
  padding: 10rpx 20rpx;

}
.list  .title,.row{
  font-size: 30rpx;
  margin:30rpx 30rpx;
  white-space: pre-line;
  overflow : hidden; 
  text-overflow: ellipsis; 
  display: -webkit-box;    
  -webkit-line-clamp: 2;   
  -webkit-box-orient: vertical;    
  word-break: break-all; 
  width:360rpx;
}
.list image{
  width:35rpx;
  height:35rpx;
  position:absolute;
  right:20rpx;
  top:30rpx;
}
.sku-price{
  color:#c00000;
}